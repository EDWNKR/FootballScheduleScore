<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.maemaem.footballschedule">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NextMatch"
            android:label="NextMatch">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".prevMatch.PrevMatch"
            android:label="PrevMatch"/>
        <activity
            android:name=".detailNextMatch.DetailNextMatch"
            android:label="Next Match Detail"/>
        <activity
            android:name=".detailPrevMatch.DetailPrevMatch"
            android:label="Previous Match Detail"/>
    </application>

</manifest>